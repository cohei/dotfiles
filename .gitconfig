[user]
	name = TANIGUCHI Kohei
	email = a.d.xvii.kal.mai@gmail.com
[color]
	ui = auto
[credential]
	helper = cache
[alias]
	co = checkout
	d = diff
	lg = log --graph --oneline --decorate --all
	dc = diff --cached
	l = log --oneline -5
	ap = add -p
	sl = stash list
	sp = stash pop
	g = grep
[status]
	showUntrackedFiles = all
[push]
	default = upstream
[rebase]
	autosquash = true
[pager]
	log = diff-highlight | less
	show = diff-highlight | less
	diff = diff-highlight | less
[rerere]
	enabled = true
