[user]
	name = TANIGUCHI Kohei
[color]
	ui = auto
[credential]
	helper = cache
[alias]
	amend = commit --amend
	ap    = add -p
	d     = diff
	dc    = diff --cached
	g     = grep
	l     = log --oneline -5
	lg    = log --graph --oneline --decorate --all
	sl    = stash list
	sp    = stash pop
	ss    = stash show -v
	s     = status -sb
	sw    = switch
	swc   = switch --create

[push]
	default = simple
[rebase]
	autosquash = true
[rerere]
	enabled = true
[help]
	autocorrect = 1
[core]
	precomposeunicode = true
	quotepath = false
	pager = diff-so-fancy | less
[diff]
	renames = copy
	compactionHeuristic = true
[url "git@github.com:"]
	pushInsteadOf = "git://github.com/"
	pushInsteadOf = "https://github.com/"
[log]
	date = iso
[ghq]
	root = ~/src
[pull]
	ff = only
[merge]
	conflictStyle = diff3
